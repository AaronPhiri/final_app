<h1>Listing orders</h1>

<table>
  <thead>
    <tr>
      <th>Order type</th>
      <th>Concept</th>
      <th>Orderer</th>
      <th>Encounter</th>
      <th>Instructions</th>
      <th>Start date</th>
      <th>Auto expire date</th>
      <th>Discontinued</th>
      <th>Discontinued date</th>
      <th>Discontinued by</th>
      <th>Discontinued reason</th>
      <th>Creator</th>
      <th>Date created</th>
      <th>Voided</th>
      <th>Voided by</th>
      <th>Date voided</th>
      <th>Void reason</th>
      <th>Patient</th>
      <th>Accession number</th>
      <th>Uuid</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @orders.each do |order| %>
      <tr>
        <td><%= order.order_type_id %></td>
        <td><%= order.concept_id %></td>
        <td><%= order.orderer %></td>
        <td><%= order.encounter_id %></td>
        <td><%= order.instructions %></td>
        <td><%= order.start_date %></td>
        <td><%= order.auto_expire_date %></td>
        <td><%= order.discontinued %></td>
        <td><%= order.discontinued_date %></td>
        <td><%= order.discontinued_by %></td>
        <td><%= order.discontinued_reason %></td>
        <td><%= order.creator %></td>
        <td><%= order.date_created %></td>
        <td><%= order.voided %></td>
        <td><%= order.voided_by %></td>
        <td><%= order.date_voided %></td>
        <td><%= order.void_reason %></td>
        <td><%= order.patient_id %></td>
        <td><%= order.accession_number %></td>
        <td><%= order.uuid %></td>
        <td><%= link_to 'Show', order %></td>
        <td><%= link_to 'Edit', edit_order_path(order) %></td>
        <td><%= link_to 'Destroy', order, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Order', new_order_path %>
